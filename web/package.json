{
  "name": "developer-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "PORT=7777 node scripts/start.js",
    "dev": "npm run start",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "csslint": "stylelint \"./src/**/*.scss\" --syntax scss --config .stylelintrc",
    "csslint:fix": "npm run csslint -- --fix",
    "tslint": "tslint \"./src/**/*.{ts,tsx}\" --project .",
    "tslint:fix": "npm run tslint -- --fix",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "fix": "NODE_ENV=development npm run csslint:fix && npm run tslint:fix && npm run prettier"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run fix"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.scss$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs",
      "ts",
      "tsx"
    ]
  },
  "prettier": {
    "semi": false,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxSingleQuote": true,
    "arrowParens": "always"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead"
        }
      ],
      [
        "@babel/preset-react"
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "@types/d3": "^5.0.1",
    "@types/dagre-d3": "^0.4.38",
    "autoprefixer": "7.1.6",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "d3-zoom": "^1.7.3",
    "dagre-d3": "^0.6.3",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "query-string": "^5.1.1",
    "raf": "3.4.0",
    "react": "^16.5.2",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.5.2",
    "react-fast-compare": "^2.0.4",
    "react-grid-layout": "^0.16.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.0",
    "react-table": "^6.8.6",
    "react-tabs": "^2.3.0",
    "react-tagsinput": "^3.19.0",
    "react-tooltip": "^3.9.2",
    "resolve": "1.6.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.118",
    "@types/query-string": "^6.1.1",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/react-tooltip": "^3.9.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "css-loader": "^1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "interpolate-html-plugin": "^3.0.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.9.3",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "react-test-renderer": "^16.7.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^9.5.0",
    "stylelint-order": "^1.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.0",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
